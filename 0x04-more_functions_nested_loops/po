#include "holberton.h"

/**
 * more_numbers - print numbers from 0 to 14
 *
 */
void more_numbers(void)
{
	int i, j;
	int divi = 0;
	int modu = 0;

	for (i = 0; i <= 10; i++)
	{
		for (j = 0; j <= 14; j++)
		{
			if (j <= 9)
			{
				_putchar (j + '0');
			}
			else
			{
				divi = j / 10;
				modu = j % 10;
				_putchar (divi + '0');
				_putchar (modu + '0');
			}

		}
		_putchar('\n');
	}
}
